   "source": [
    "T = int(input())\n",
    "\n",
    "for test_case in range(1, T + 1):\n",
    "    result = 0\n",
    "    row = list(map(int,input().split()))\n",
    "\n",
    "    for i in row:\n",
    "        if i % 2 == 1:\n",
    "            result += i\n",
    "    print('#%d %d' % (test_case, result))"
   ]
  }
